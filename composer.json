{
    "name": "digitalpulsebe/craft-utm-tracker",
    "description": "Track landing query parameters in user session",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "utm tracker"
    ],
    "support": {
        "docs": "https://github.com/digitalpulsebe/craft-utm-tracker/blob/master/README.md",
        "issues": "https://github.com/digitalpulsebe/craft-utm-tracker/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Pulse",
            "homepage": "https://www.digitalpulse.be/"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0-alpha.1"
    },
    "autoload": {
        "psr-4": {
          "digitalpulsebe\\utmtracker\\": "src/"
        }
    },
    "extra": {
        "name": "UTM Tracker",
        "handle": "utm-tracker",
        "developer": "Digital Pulse",
        "developerUrl": "https://www.digitalpulse.be/",
        "documentationUrl": "https://github.com/digitalpulsebe/craft-utm-tracker/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/digitalpulsebe/craft-utm-tracker/master/CHANGELOG.md",
        "components": {
            "utmTrackerService": "digitalpulsebe\\utmtracker\\services\\UtmTrackerService"
        },
        "class": "digitalpulsebe\\utmtracker\\UtmTracker"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "require-dev": {
        "verbb/formie": "^3.0.0-beta.1"
    }
}
